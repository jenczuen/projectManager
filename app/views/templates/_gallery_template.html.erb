<script type="x-handlebars-template" id="gallery-template">
	<h1>{{ category }} - galeria</h1>

	{{#each images}}
		<h2>{{ this.title }}</h2>
		<center>
			<p>{{ this.description }}</p>
			<img src={{ this.url }} alt={{ this.title }}> 
		</center>
	{{/each}}

	<a href="#" onclick={{ function_name }}>Add image</a>
</script>